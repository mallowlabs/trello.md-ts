  "homepage": "https://github.com/mallowlabs/trello.md-ts#readme",
  "devDependencies": {
    "@types/chrome": "^0.0.224",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "ts-loader": "^9.4.4",
    "typescript": "^5.1.6",
    "webpack": "^5.76.2",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "trello-web": "^1.0.3"
