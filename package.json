{
  "name": "trello.md-ts",
  "version": "1.1.0",
  "description": "Copy trello board information to clipboard as markdown format.",
  "scripts": {
    "build": "webpack --mode production",
    "lint": "run-p lint:*",
    "lint:prettier": "prettier --check src/",
    "lint:eslint": "eslint \"src/**/*.ts\"",
    "fix": "run-p fix:*",
    "fix:prettier": "prettier -w src/",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mallowlabs/trello.md-ts.git"
  },
  "author": "mallowlabs",
  "license": "MIT",
  "homepage": "https://github.com/mallowlabs/trello.md-ts#readme",
  "devDependencies": {
    "@types/chrome": "^0.0.287",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.4.2",
    "ts-loader": "^9.5.1",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.1",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "trello-web": "^1.0.3"
  }
}
